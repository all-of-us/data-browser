{
  "name": "aou-public-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --sourcemaps=false",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "codegen": "yarn run merge-swagger && yarn run codegen-clean && yarn run public-codegen-swagger && yarn run public-codegen-swagger-fetch && yarn run public-post-codegen",
    "merge-swagger": "../public-api/build.rb merge-yaml",
    "codegen-clean": "rm -rf src/generated src/publicGenerated",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts",
    "public-codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../public-api/src/main/resources/merged.yaml --output src/publicGenerated --additional-properties ngVersion=11.0.0",
    "public-post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/publicGenerated/variables.ts",
    "public-codegen-swagger-fetch": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-fetch --input-spec ../public-api/src/main/resources/merged.yaml --output src/publicGenerated/fetch/ --additional-properties ngVersion=11.0.0",
    "coverage": "ng test --sourcemaps=false --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@clr/angular": "^4.0.8",
    "@clr/icons": "^4.0.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.5",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular-font-awesome": "^3.1.2",
    "angular2-highcharts": "^0.5.5",
    "core-js": "^2.5.3",
    "enhanced-resolve": "3.3.0",
    "es6-promise": "^4.2.8",
    "font-awesome": "^4.7.0",
    "highcharts": "^6.0.4",
    "immutable": "^4.0.0-rc.9",
    "moment": "^2.20.1",
    "mutationobserver-shim": "^0.3.2",
    "ngx-pagination": "^4.1.0",
    "ngx-responsive": "^9.0.3",
    "nouislider": "10.0.0",
    "popper.js": "^1.13.0",
    "portable-fetch": "^3.0.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "stackdriver-errors-js": "^0.4.0",
    "stacktrace-js": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular-devkit/core": "0.4.7",
    "@angular-devkit/schematics": "0.4.7",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/language-service": "^11.0.5",
    "@types/google.analytics": "0.0.36",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^9.4.0",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-webdriver-launcher": "^1.0.5",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "^5.9.1",
    "typescript": "4.0.5"
  }
}
