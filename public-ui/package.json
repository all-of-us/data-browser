{
  "name": "aou-public-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --sourcemaps=false",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "codegen": "yarn run merge-swagger && yarn run codegen-clean && yarn run public-codegen-swagger && yarn run public-post-codegen",
    "merge-swagger": "../public-api/build.rb merge-yaml",
    "codegen-clean": "rm -rf src/generated src/publicGenerated",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts",
    "public-codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../public-api/src/main/resources/merged.yaml --output src/publicGenerated --additional-properties ngVersion=2",
    "public-post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/publicGenerated/variables.ts",
    "coverage": "ng test --sourcemaps=false --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/forms": "^10.0.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@clr/angular": "^4.0.0",
    "@clr/icons": "^4.0.0",
    "@clr/ui": "^4.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@webcomponents/custom-elements": "^1.4.2",
    "angular-2-local-storage": "^3.0.2",
    "angular-font-awesome": "^3.1.2",
    "angular2-highcharts": "^0.5.5",
    "core-js": "^3.6.5",
    "enhanced-resolve": "4.3.0",
    "font-awesome": "^4.7.0",
    "highcharts": "^6.0.4",
    "immutable": "^4.0.0-rc.9",
    "moment": "^2.20.1",
    "mutationobserver-shim": "^0.3.2",
    "ngx-pagination": "^4.1.0",
    "ngx-responsive": "^6.0.0",
    "nouislider": "10.0.0",
    "popper.js": "^1.13.0",
    "rxjs": "^5.5.6",
    "stackdriver-errors-js": "^0.4.0",
    "stacktrace-js": "^2.0.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular-devkit/core": "10.0.8",
    "@angular-devkit/schematics": "10.0.8",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/google.analytics": "0.0.40",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.6.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-webdriver-launcher": "^1.0.8",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0"
  }
