{
  "name": "aou-public-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --sourcemaps=false",
    "lint": "yarn eslint src --ext .tsx",
    "e2e": "ng e2e",
    "codegen": "yarn run codegen-clean && yarn run public-codegen-swagger && yarn run public-codegen-swagger-fetch && yarn run public-post-codegen",
    "codegen-clean": "rm -rf src/generated src/publicGenerated",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts",
    "public-codegen-swagger": "openapi-generator-cli generate -g typescript-angular -i ../public-api/src/main/resources/public-api.yaml -o src/publicGenerated --additional-properties ngVersion=10.2.4",
    "public-post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/publicGenerated/variables.ts",
    "public-codegen-swagger-fetch": "openapi-generator-cli generate -g typescript-fetch -i ../public-api/src/main/resources/public-api.yaml -o src/publicGenerated/fetch/ -p generateAliasAsModel=true --additional-properties=typeScriptThreePlus=true,enumPropertyNaming=UPPERCASE,useSingleRequestParameter=false,prefixParameterInterfaces=true",
    "coverage": "ng test --sourcemaps=false --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^19.0.0",
    "@angular/animations": "^19.2.18",
    "@angular/cdk": "^19.0.0",
