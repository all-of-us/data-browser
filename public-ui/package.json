    "nyc": "^15.1.0",
    "portable-fetch": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-paginate": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "stackdriver-errors-js": "^0.4.0",
    "stacktrace-js": "^2.0.0",
    "url": "^0.11.0",
    "yarn": "^1.22.10",
