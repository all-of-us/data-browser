<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <changeSet author="srushtigangireddy" id="changelog-1" runAlways="true" context="local or cloud">

        <validCheckSum>ANY</validCheckSum>

        <loadData tableName="workspace" file="csv/workspace.csv" encoding="UTF-8" quotchar='"'>
            <column name="workspace_id" type="NUMERIC"/>
            <column name="name" type="STRING"/>
            <column name="access_level" type="NUMERIC"/>
            <column name="creation_time" type="DATE"/>
            <column name="archival_time" type="DATE"/>
            <column name="research_purpose" type="STRING"/>
            <column name="reason_for_all_of_us" type="STRING"/>
            <column name="anticipated_findings" type="STRING"/>
            <column name="intended_study" type="STRING"/>
        </loadData>

    </changeSet>
</databaseChangeLog>
