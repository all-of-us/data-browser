        CASE WHEN PREDICTED_COPY_GAIN IS NOT NULL THEN CAST(PREDICTED_COPY_GAIN AS STRING) ELSE NULL END,
        CASE WHEN PREDICTED_DUP_PARTIAL IS NOT NULL THEN CAST(PREDICTED_DUP_PARTIAL AS STRING) ELSE NULL END,
