        CASE WHEN PREDICTED_LOF IS NOT NULL THEN CAST(PREDICTED_LOF AS STRING) ELSE NULL END,
        CASE WHEN PREDICTED_MSV_EXON_OVERLAP IS NOT NULL THEN CAST(PREDICTED_MSV_EXON_OVERLAP AS STRING) ELSE NULL END,
