  `aou-db-prod.2024q3r2_genomics.aou_sv_vcf_7_1_processed`;

CREATE OR REPLACE TABLE `aou-db-prod.2024q3r2_genomics.aou_sv_vcf_7_1_processed` AS
-- Step 1: Identify positions (CHROM + POS) that are unique
WITH single_cnt_ids AS (
  SELECT chrom, pos, COUNT(*) AS cnt
  FROM `aou-db-prod.2024q3r2_genomics.aou_sv_vcf_7_1_processed`
  GROUP BY chrom, pos
  HAVING cnt = 1
),

-- Step 2: Identify positions that have duplicates (i.e., same CHROM + POS appears multiple times)
multiple_cnt_ids AS (
  SELECT chrom, pos, COUNT(*) AS cnt
  FROM `aou-db-prod.2024q3r2_genomics.aou_sv_vcf_7_1_processed`
  GROUP BY chrom, pos
  HAVING cnt > 1
),

-- Step 3: Assign variant_id to unique CHROM + POS rows
-- No suffix needed since there's only one record for each
single_random_ids AS (
  SELECT DISTINCT
    a.* EXCEPT (variant_id),
    CONCAT(SUBSTR(a.chrom, 4), '-', CAST(a.pos AS STRING)) AS variant_id
  FROM `aou-db-prod.2024q3r2_genomics.aou_sv_vcf_7_1_processed` a
  JOIN single_cnt_ids b
    ON a.chrom = b.chrom AND a.pos = b.pos
),

-- Step 4: Assign variant_id to duplicated CHROM + POS rows
-- Use letter suffix (a, b, c, ...) to ensure uniqueness
multiple_random_ids AS (
  SELECT
    a.* EXCEPT (variant_id),
    CONCAT(
      SUBSTR(a.chrom, 4), '-',
      CAST(a.pos AS STRING),
      CHR(96 + ROW_NUMBER() OVER (PARTITION BY a.chrom, a.pos ORDER BY a.variant_id))
    ) AS variant_id
  FROM `aou-db-prod.2024q3r2_genomics.aou_sv_vcf_7_1_processed` a
  JOIN multiple_cnt_ids b
    ON a.chrom = b.chrom AND a.pos = b.pos
)

-- Step 5: Combine both sets into a single unified table
SELECT
  chrom,
  pos,
  CASE
    WHEN chrom = chr2 OR chr2 IS NULL THEN CONCAT(SUBSTR(a.chrom, 4), ':', CAST(a.pos AS STRING), '-', CAST(a.end AS STRING))
    ELSE CONCAT(SUBSTR(a.chrom, 4), ':', CAST(a.pos AS STRING), '-', CAST(a.chr2 AS STRING), ':', CAST(a.end AS STRING))
  END AS position,
  variant_type, filter, allele_count, allele_number, chr2, cpx_intervals, cpx_type,
  a.end, end2, predicted_breakend_exonic, predicted_copy_gain, predicted_dup_partial,
  predicted_intergenic, predicted_intragenic_exon_dup, predicted_intronic, predicted_inv_span,
  predicted_lof, predicted_msv_exon_overlap, predicted_nearest_tss, predicted_partial_exon_dup,
  predicted_promoter, predicted_tss_dup, predicted_utr, size, allele_frequency, homozygote_count,
  afr_an, afr_ac, afr_af, afr_n_homalt, amr_an, amr_ac, amr_af, amr_n_homalt,
  eas_an, eas_ac, eas_af, eas_n_homalt, eur_an, eur_ac, eur_af, eur_n_homalt,
  mid_an, mid_ac, mid_af, mid_n_homalt, oth_an, oth_ac, oth_af, oth_n_homalt,
  sas_an, sas_ac, sas_af, sas_n_homalt, consequence, consequence_genes, genes,
  variant_id AS variant_id_vcf,
  variant_id AS variant_id
FROM single_random_ids a

UNION ALL

SELECT
  chrom,
  pos,
  CASE
    WHEN chrom = chr2 OR chr2 IS NULL THEN CONCAT(SUBSTR(b.chrom, 4), ':', CAST(b.pos AS STRING), '-', CAST(b.end AS STRING))
    ELSE CONCAT(SUBSTR(b.chrom, 4), ':', CAST(b.pos AS STRING), '-', CAST(b.chr2 AS STRING), ':', CAST(b.end AS STRING))
  END AS position,
  variant_type, filter, allele_count, allele_number, chr2, cpx_intervals, cpx_type,
  b.end, end2, predicted_breakend_exonic, predicted_copy_gain, predicted_dup_partial,
  predicted_intergenic, predicted_intragenic_exon_dup, predicted_intronic, predicted_inv_span,
  predicted_lof, predicted_msv_exon_overlap, predicted_nearest_tss, predicted_partial_exon_dup,
  predicted_promoter, predicted_tss_dup, predicted_utr, size, allele_frequency, homozygote_count,
  afr_an, afr_ac, afr_af, afr_n_homalt, amr_an, amr_ac, amr_af, amr_n_homalt,
  eas_an, eas_ac, eas_af, eas_n_homalt, eur_an, eur_ac, eur_af, eur_n_homalt,
  mid_an, mid_ac, mid_af, mid_n_homalt, oth_an, oth_ac, oth_af, oth_n_homalt,
  sas_an, sas_ac, sas_af, sas_n_homalt, consequence, consequence_genes, genes,
  variant_id AS variant_id_vcf,
  variant_id AS variant_id
FROM multiple_random_ids b;